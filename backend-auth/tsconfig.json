{
  "compilerOptions": {
    "target": "ESNext",
  "module": "ESNext",
  "jsx": "react-jsx", 
  "moduleResolution": "node",
  "isolatedModules": true,
    "rootDir": "src",
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }
}